{
  "name": "kz-vercel-template",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "Kai Zau",
    "email": "oss@kaizau.com",
    "url": "https://kaizau.com"
  },
  "repository": "",
  "private": true,
  "browserslist": "defaults, not IE 11",
  "parcelIgnore": [
    "site/_.+"
  ],
  "scripts": {
    "start": "PORT=8888 npm run parcel",
    "netlify": "PORT=8887 netlify dev",
    "vercel": "vercel dev --listen 8888",
    "parcel": "rimraf dist/ && parcel --port $PORT 'site/**/*.html'",
    "build": "rimraf dist/ && parcel build 'site/**/*.html' --no-source-maps --no-cache",
    "lint": "prettier --ignore-path .gitignore --write . && eslint --fix .",
    "test": "jest --watch"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^6.1.5"
  },
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^29.5.0",
    "parcel": "^2.8.3",
    "parcel-resolver-ignore": "^2.1.3",
    "postcss": "^8.4.23",
    "postcss-modules": "^6.0.0",
    "posthtml": "^0.16.6",
    "posthtml-expressions": "^1.11.0",
    "posthtml-modules": "^0.9.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.2"
  }
}
