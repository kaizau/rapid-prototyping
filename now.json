{
  "name": "",
  "alias": "",
  "version": 2,

  "build": {
    "env": {
      "EXAMPLE_BUILD_VAR": "Production EXAMPLE_BUILD_VAR from now.json"
    }
  },

  "env": {
    "EXAMPLE_FUNCTION_VAR": "Production EXAMPLE_FUNCTION_VAR from now.json"
  },

  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    },
    {
      "src": "functions/*.js",
      "use": "@now/node"
    }
  ],

  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/functions/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
